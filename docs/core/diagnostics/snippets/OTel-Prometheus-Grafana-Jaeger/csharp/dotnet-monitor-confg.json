{
  "$schema": "https://aka.ms/dotnet-monitor-schema",
  "DefaultProcess": {
    "Filters": [
      {
        "Key": "ProcessName",
        "Value": "OTel-Prometheus-Graphana-Yaeger"
      }
    ]
  },
  "Metrics": {
    "IncludeDefaultProviders": true,
    "Providers": [
      {
        "ProviderName": "OtPrGrYa.Sample"
      },
      {
        "ProviderName": "Microsoft.AspNetCore.Hosting",
        "MetricType": "Meter"
      }
    ]
  }
}
