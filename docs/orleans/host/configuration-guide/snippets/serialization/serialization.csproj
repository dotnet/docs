<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <!-- Additional nullable warnings specific to this project -->
    <NoWarn>$(NoWarn);CS8602;CS8603</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Orleans.Sdk" Version="$(OrleansVersion)" />
    <PackageReference Include="Microsoft.Orleans.Server" Version="$(OrleansVersion)" />
    <PackageReference Include="Microsoft.Orleans.Serialization.MessagePack" Version="$(OrleansVersion)" />
    <PackageReference Include="Microsoft.Orleans.Persistence.AzureStorage" Version="$(OrleansVersion)" />
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="$(HostingVersion)" />
    <PackageReference Include="MessagePack" Version="3.1.4" />
  </ItemGroup>
</Project>
