<!-- Displays the different animation examples. -->
<Window  
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    x:Class="keyframes_markup.SampleViewer"
    Title="KeyFrames Example (Markup)">

    
    <DockPanel>
    <TabControl Background="White">
      
      <TabItem Header="ChainedBitmapSources">  
        <Frame Source="ChainedBitmapSourcesExample.xaml" />
      </TabItem> 
      <TabItem Header="FormatConvertedBitmap">  
        <Frame Source="FormatConvertedBitmapExample.xaml" />
      </TabItem>
      <TabItem Header="TransformedBitmapExample">  
        <Frame Source="TransformedBitmapExample.xaml" />
      </TabItem>     
    </TabControl>
   
    </DockPanel>
</Window>
