<!--<SnippetFrameHostPageXAML1>-->
<Page 
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  WindowTitle="Page that Hosts a Frame"
  WindowWidth="250"
  WindowHeight="250">
  <!--</SnippetFrameHostPageXAML1>-->
  <DockPanel>
    <TextBlock FontSize="15" Margin="5,5,5,5" FontWeight="Bold" DockPanel.Dock="Top">Hello, Page with a Frame!</TextBlock>
    <Border Padding="5" Margin="5,0,5,5" BorderBrush="Black" BorderThickness="2">
      <!--<SnippetFrameHostPageXAML2>-->
      <Frame Source="FramePage1.xaml" />
      <!--</SnippetFrameHostPageXAML2>-->
    </Border>
  </DockPanel>
  <!--<SnippetFrameHostPageXAML3>-->
</Page>
<!--</SnippetFrameHostPageXAML3>-->