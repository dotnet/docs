<Page  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  x:Class="SDKSample.SampleViewer"
  xmlns:Examples="clr-namespace:SDKSample" >
  <DockPanel Background="White">
    <TabControl Name="sampleSelector">
      <TabItem Header="BeginChange/EndChange Example">
        <Frame Name="MyBeginChangeEndChangeExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="SpellCheck Example">
        <Frame Name="MySpellCheckExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="TextBox Example">
        <Frame Name="MyTextBoxExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="CharacterCasing">
        <Frame Name="MyCharacterCasingExampleFrame" Background="White" />
      </TabItem>
    </TabControl>
  </DockPanel>
</Page>
