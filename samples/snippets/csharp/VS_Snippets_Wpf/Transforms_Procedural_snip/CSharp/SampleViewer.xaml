<Page  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  x:Class="SDKSample.SampleViewer"
  xmlns:examples="clr-namespace:SDKSample" >
  <DockPanel Background="White">
    <TabControl Name="sampleSelector">
      <TabItem Header="MultipleTransforms Example">
        <Frame Name="MyMultipleTransformsExampleFrame" Background="White" />
      </TabItem> 
      <TabItem Header="RotateTransform Example">
        <Frame>
          <Frame.Content>
            <examples:RotateTransformExample />
          </Frame.Content>
        </Frame>
      </TabItem>       
    </TabControl>
  </DockPanel>
</Page>
