<Page  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  x:Class="SDKSample.SampleViewer"
  xmlns:Examples="clr-namespace:SDKSample" >
  <DockPanel Background="White">
    <TabControl Name="sampleSelector">
      <TabItem Header="AutoLayoutContent Example">
        <Frame Name="MyAutoLayoutContentExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="Reflection Example">
        <Frame Name="MyReflectionExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="Fill with Video">
        <Frame Name="MyPaintWithVideoExampleFrame" Background="White" />
      </TabItem>
    </TabControl>
  </DockPanel>
</Page>
