<Page  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  x:Class="SDKSample.SampleViewer" >
  <DockPanel Background="White">
    <TabControl Name="sampleSelector">
      <TabItem Header="FigureExample">
        <Frame Name="MyFigureExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="InlineUIContainerExample">
        <Frame Name="MyInlineUIContainerExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="SimpleFlowExample">
        <Frame Name="MySimpleFlowExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="ParagraphExample">
        <Frame Name="MyParagraphExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="SectionExample">
        <Frame Name="MySectionExampleFrame" Background="White" />
      </TabItem>
      <TabItem Header="ListExample">
        <Frame Name="MyListExampleFrame" Background="White" />
      </TabItem>
    </TabControl>
  </DockPanel>
</Page>
