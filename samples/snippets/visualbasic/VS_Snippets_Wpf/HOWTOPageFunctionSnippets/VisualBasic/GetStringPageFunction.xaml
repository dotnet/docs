<PageFunction
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib" 
    x:Class="UsingPageFunctionsSample.GetStringPageFunction"
    x:TypeArguments="sys:String"
    Title="GetStringPageFunction">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <Label>String:</Label>
            <TextBox Name="stringTextBox"></TextBox>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <Button Name="okButton" Click="okButton_Click">OK</Button>
            <Button Name="cancelButton" Click="cancelButton_Click">Cancel</Button>
        </StackPanel>
    </StackPanel>
</PageFunction>
