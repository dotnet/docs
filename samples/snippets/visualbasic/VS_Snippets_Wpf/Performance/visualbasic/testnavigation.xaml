<Page xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      x:Class="SDKSample.TestNavigation"
      Background="LightBlue"
      Loaded="Init">
  
  <StackPanel Orientation="Vertical" Margin="20, 20, 20, 20">
    <TextBlock FontWeight="Bold">
      Page 1
    </TextBlock>

    <TextBlock>Assign new URI</TextBlock>
    <Button Click="buttonGoToUri"
            HorizontalAlignment="Left"
            Height="30"
            Width="150"
            Content="Go To URI"
            Margin="10,0,10,10"/>

    <TextBlock>Assign new Content</TextBlock>
    <Button Click="buttonGoNewObject"
            HorizontalAlignment="Left"
            Height="30"
            Width="150"
            Content="Go To New Page"
            Margin="10,0,10,10"/>
    
  </StackPanel>
</Page>

