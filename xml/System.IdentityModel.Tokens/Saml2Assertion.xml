      <MemberSignature Language="F#" Value="member this.CanWriteSourceData : bool" Usage="System.IdentityModel.Tokens.Saml2Assertion.CanWriteSourceData" />
      <MemberType>Property</MemberType>
      <AssemblyInfo>
