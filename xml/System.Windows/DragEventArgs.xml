      <MemberSignature Language="C#" Value="public System.Windows.DragDropEffects AllowedEffects { get; }" />
      <MemberSignature Language="ILAsm" Value=".property instance valuetype System.Windows.DragDropEffects AllowedEffects" />
      <MemberSignature Language="DocId" Value="P:System.Windows.DragEventArgs.AllowedEffects" />
